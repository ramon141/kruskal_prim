package utils;

public class No<T> {

	private T value;
	private No<T> right;
	private No<T> left;
	
	public No(T value) {
		this.value = value;
		this.right = null;
		this.left = null;
	}

	public No<T> getRight() {
		return right;
	}

	public void setRight(No<T> right) {
		this.right = right;
	}

	public No<T> getLeft() {
		return left;
	}

	public void setLeft(No<T> left) {
		this.left = left;
	}

	public T getValue() {
		return value;
	}
	
	public T getValueToCompare() {
		return value;
	}

	public void setValue(T value) {
		this.value = value;
	}
	
	
}
